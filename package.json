{
  "private": true,
  "name": "hafa-next",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "fmt": "prettier --write --cache 'pages/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' 'styles/**/*.css' '*.{json,js,ts}'",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@types/node": "^18.0.0",
    "@types/react": "18.0.14",
    "acorn": "^8.7.1",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.1.6",
    "next": "^12.1.6",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.4"
  },
  "resolutions": {
    "minimist": "^1.2.6",
    "minimatch": "^3.0.5"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  },
  "packageManager": "yarn@3.2.4"
}
