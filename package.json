{
  "private": true,
  "name": "hafa-next",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "fmt": "prettier --write --cache 'pages/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' 'styles/**/*.css' '*.{json,js,ts}'",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@types/node": "^18.11.18",
    "@types/react": "18.0.27",
    "acorn": "^8.8.2",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "next": "^13.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "json5": "^2.2.2"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  },
  "packageManager": "yarn@3.2.4"
}
