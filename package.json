{
  "private": true,
  "name": "hafa-next",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "fmt": "prettier --write --cache 'pages/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' 'styles/**/*.css' '*.{json,js,ts}'",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@types/node": "^20.3.2",
    "@types/react": "18.2.14",
    "acorn": "^8.9.0",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.4.7",
    "next": "^13.4.7",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1.6"
  },
  "resolutions": {
    "json5": "^2.2.2"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  },
  "packageManager": "yarn@3.6.0"
}
