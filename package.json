{
  "private": true,
  "name": "hafa-next",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "fmt": "prettier --write --cache 'pages/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' 'styles/**/*.css' '*.{json,js,ts}'",
    "lint": "next lint && tsc"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@types/node": "^18.11.9",
    "@types/react": "18.0.25",
    "acorn": "^8.8.1",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.27.0",
    "eslint-config-next": "^13.0.3",
    "next": "^13.0.3",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "minimist": "^1.2.6",
    "minimatch": "^3.0.5"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "fixme"
          ]
        }
      ]
    }
  },
  "packageManager": "yarn@3.2.4"
}
